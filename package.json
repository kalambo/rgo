{
  "name": "rgo",
  "version": "0.3.13",
  "description": "Realtime graph orchestrator",
  "author": "Jon Whitehead",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "prettier": "prettier --write \"{src,test}/**/*.ts\"",
    "clean": "rimraf lib/*",
    "build": "yarn run clean && tsc",
    "test": "jest",
    "prepare": "yarn run test && yarn run build"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@types/diff": "^3.5.1",
    "diff": "^3.5.0",
    "keys-to-object": "^0.2.0"
  }
}
